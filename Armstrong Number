/* To check for Armstrong Number*/

#include<stdio.h>  
int main()   
{
 int a;
 printf("Enter the number:") ;    
 scanf("%d", &a) ;  
 armstrong (a);  
 return 1 ;  
}   
void armstrong(int a)
{    
 int b, n, sum=0;
 n=a;
   while(a>0)    
   {    
     b=a%10 ;    
     sum=sum+(b*b*b) ;    
     a=a/10 ;    
     }    
if(sum==n)    
      printf("Armstrong  number ") ;    
else    
      printf("Not Armstrong number") ;    
}
