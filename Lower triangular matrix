#include<stdio.h>
int main()
{
int a[3][3];
int i, j, t;
printf ("Enter elements: ");
for(i=0; i<3; i++)
   for(j=0; j<3; j++)
         scanf("%d", &a[i][j]);
for(i=0; i<3; i++)
   for(j=i+1; j<3; j++)
        {
         if(a[i][j]==0)
             t=0; 
             else
              t=1;
          }
if(t==0)
   printf ("\n Lower triangular matrix");
else
   printf ("\n Not Lower triangular matrix");
return 1;
}






